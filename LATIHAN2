package tugasM4;

/**
 *
 * @author ROG
 */
public class StatementDoWhile {
 public static void main(String[] args){
     
     //membuat variabel
     int i = 0;
     
     do {
         System.out.println("Perulangan ke-" + i);
         i++;
     } while ( i <= 10);
 }   
}
