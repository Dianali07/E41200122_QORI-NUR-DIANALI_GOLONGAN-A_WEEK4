package tugasM4;

import java.util.Scanner;

/**
 *
 * @author ROG
 */
public class For {
      public static void main(String[] args){
        
         int n;
        Scanner scan = new Scanner(System.in);
        //perintah awal
        System.out.print("Masukkan Nilai Angka Terbesar adalah = ");
        n=scan.nextInt();
        //masukkan perulangan for
        for(int i=2; i <= n; i += 2) {
            System.out.print(i + " ");
        }
      }
}
       
